#include <bits/stdc++.h>

using namespace std;

bool isValidSudoku(vector<vector<char>>& board){
	unordered_set<string> seen;
	for(int i=0; i<9; i++){
		for(int j=0; j<9; j++){
			if(board[i][j] != '0'){
				string rowKey = "row" + to_string(i) + board[i][j];
				string colKey = "col" + to_string(j) + board[i][j];
				string boxKey = "box" + to_string((i/3)*3+j/3) + board[i][j];
				if (!seen.insert(rowKey).second ||
					!seen.insert(colKey).second ||
					!seen.insert(boxKey).second){
						return false;
					}
			}
		}
	}
	
	return true;
}

int main(){
	vector<vector<char>> board = {
        {'5', '3', '0', '0', '7', '0', '0', '0', '0'},
        {'6', '0', '0', '1', '9', '5', '0', '0', '0'},
        {'0', '9', '8', '0', '0', '0', '0', '6', '0'},
        {'8', '0', '0', '0', '6', '0', '0', '0', '3'},
        {'4', '0', '0', '8', '0', '3', '0', '0', '1'},
        {'7', '0', '0', '0', '2', '0', '0', '0', '6'},
        {'0', '6', '0', '0', '0', '0', '2', '8', '0'},
        {'0', '0', '0', '4', '1', '9', '0', '0', '5'},
        {'0', '0', '0', '0', '8', '0', '0', '7', '9'}
	};
// if asking user input
//	vector<vector<char>> board(9, vector<char>(90));
//	cout << "Enter the Sudoku board: " << endl;
//	for(int i=0; i<9; i++){
//		for(int j=0; j<9; j++){
//			cin >> board[i][j];
//		}
//	}
	
	bool result = isValidSudoku(board);
	
	cout << (result? "This is a valid Sudoku board." : "This is not a valid Sudoku board.") << endl;
	return 0;
}
